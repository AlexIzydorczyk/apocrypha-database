<script type="text/javascript">
let bookletVue;
$(document).ready(function(){
	bookletVue = new Vue({
		name: 'bookletVue',
		el: '#booklet-form',
		data: {
			date_exact: <%= booklet.date_exact ? "true" : "false" %>,
		},
		mounted(){
			runTomSelects();
		},
	});
});
function runTomSelects() {
	new TomSelect("#person_reference_id", ts_sort_text_asc_max_null);
	new TomSelect("#booklet_genesis_location_id",ts_sort_text_asc_max_1);
	new TomSelect("#booklet_genesis_institution_id", ts_sort_text_asc_max_1);
	new TomSelect("#booklet_genesis_religious_order_id", ts_sort_text_asc_max_1);
	new TomSelect("#booklet_content_type", ts_sort_text_asc_max_1_create);
};
</script>
